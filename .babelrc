{
  "presets": [
    ["env"],
    "react"
  ],
  "plugins": [
    ["babel-plugin-transform-require-ignore", {
      "extensions": [".less"]
    }],
    ["module-resolver", {
      "alias": {
        "react": "preact/compat",
        "@app-elements/equal": "./util/equal",
        "^@app-elements/(.+)": "./components/\\1",
        "": "./src"
      }
    }],
    ["jsx-import/src/index.js", {
      "identifier": "Preact",
      "moduleName": "preact"
    }],
    ["transform-react-jsx", {"pragma": "Preact.h"}]
  ]
}
